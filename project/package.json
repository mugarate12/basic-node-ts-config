{
  "name": "base-ts-node-config",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --poll --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "npm run build&&node build/src/server.js",
    "test": "SET NODE_ENV=test&&jest",
    "test:docker": "jest",
    "docker:test": "docker-compose -f ./docker-compose.test.yml up",
    "docker:test:build": "docker-compose -f ./docker-compose.test.yml up --build",
    "docker:dev": "docker-compose -f ./docker-compose.dev.yml up",
    "docker:dev:build": "docker-compose -f ./docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose -f ./docker-compose.yml up",
    "docker:prod:build": "docker-compose -f ./docker-compose.yml up --build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "redis": "^4.3.1",
    "run-script-os": "^1.1.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.8",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.14",
    "@types/supertest": "^2.0.12",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  }
}
